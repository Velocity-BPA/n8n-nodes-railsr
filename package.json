{
  "name": "n8n-nodes-railsr",
  "version": "1.0.0",
  "description": "A comprehensive n8n community node for Railsr embedded finance platform providing 23 resources and 150+ operations for banking, payments, cards, FX, and compliance workflows.",
  "license": "BUSL-1.1",
  "author": {
    "name": "Velocity BPA",
    "email": "licensing@velobpa.com",
    "url": "https://velobpa.com"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Velocity-BPA/n8n-nodes-railsr.git"
  },
  "homepage": "https://github.com/Velocity-BPA/n8n-nodes-railsr",
  "keywords": [
    "n8n",
    "n8n-community-node-package",
    "railsr",
    "embedded-finance",
    "banking-as-a-service",
    "baas",
    "payments",
    "sepa",
    "faster-payments",
    "bacs",
    "swift",
    "cards",
    "card-issuing",
    "fx",
    "foreign-exchange",
    "kyc",
    "kyb",
    "compliance",
    "ledger",
    "multi-currency"
  ],
  "main": "dist/nodes/Railsr/Railsr.node.js",
  "n8n": {
    "n8nNodesApiVersion": 1,
    "credentials": [
      "dist/credentials/RailsrApi.credentials.js"
    ],
    "nodes": [
      "dist/nodes/Railsr/Railsr.node.js",
      "dist/nodes/Railsr/RailsrTrigger.node.js"
    ]
  },
  "scripts": {
    "build": "tsc && gulp build:icons",
    "dev": "tsc --watch",
    "lint": "eslint . --ext .ts",
    "lint:fix": "eslint . --ext .ts --fix",
    "format": "prettier --write \"**/*.ts\"",
    "test": "jest",
    "test:coverage": "jest --coverage",
    "prepublishOnly": "npm run build"
  },
  "files": [
    "dist"
  ],
  "devDependencies": {
    "@types/jest": "^29.5.0",
    "@types/node": "^20.10.0",
